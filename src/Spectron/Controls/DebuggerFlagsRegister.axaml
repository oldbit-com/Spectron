<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OldBit.Spectron.Controls.DebuggerFlagsRegister">

    <Grid ColumnDefinitions="20,20,20,20,20,20,20,20" RowDefinitions="20,20">
        <TextBlock Grid.Row="0" Grid.Column="0" Text="S" />
        <TextBlock Grid.Row="0" Grid.Column="1" Text="Z" />
        <TextBlock Grid.Row="0" Grid.Column="2" Text="5" />
        <TextBlock Grid.Row="0" Grid.Column="3" Text="H" />
        <TextBlock Grid.Row="0" Grid.Column="4" Text="3" />
        <TextBlock Grid.Row="0" Grid.Column="5" Text="P" />
        <TextBlock Grid.Row="0" Grid.Column="6" Text="N" />
        <TextBlock Grid.Row="0" Grid.Column="7" Text="C" />

        <TextBlock Grid.Row="1" Grid.Column="0" Text="×" />
        <TextBlock Grid.Row="1" Grid.Column="1" Text="●" />
        <TextBlock Grid.Row="1" Grid.Column="2" Text="×" />
        <TextBlock Grid.Row="1" Grid.Column="3" Text="●" />
        <TextBlock Grid.Row="1" Grid.Column="4" Text="×" />
        <TextBlock Grid.Row="1" Grid.Column="5" Text="●" />
        <TextBlock Grid.Row="1" Grid.Column="6" Text="×" />
        <TextBlock Grid.Row="1" Grid.Column="7" Text="●" />
    </Grid>
</UserControl>
